            padding: 6px 11px 6px 31px; 
            color: #FFF; 
            background: #000 url('../img/arrow_white.gif') no-repeat 6px 50%; 
        }

@include media-query(lap) { 

    .page--course {
        .pg-title { min-height: 46px; }

        .intro { 
            margin: -30px 0 50px 0;
            width: 90%; 
        }
    }

        .l-title { 
            @include font-size(12px);
            font-weight: normal;
        }

        .box--img { padding-left: 0; }

            .box--cont p { clear: both; }

        .form-express-interest {

            input { 
                max-width: 220px;
            }

            textarea { width: 320px; }
        }

        .checkboxes-box { margin-left: 145px; }

}

@include media-query(desk) { 

    .page--course {
        .pg-title { 
            min-height: 46px;
        }

        .intro { 
            margin: -30px 0 50px 0;
            width: 90%; 
        }
    }

        .l-title { 
            @include font-size(12px);
            font-weight: normal;
        }

    .sub-title {
        @include font-size(12px); 
        font-weight: normal;
        margin-top: -6px; 
    }

        .box { height: 239px;  }

            // .box--img { margin-left: -24px; }

            .box--cont p { clear: both; }

        .form-express-interest {
            input { 
                max-width: 220px;
            }

            textarea { 
                width: 320px; 
                height: 200px;
            }
        }

        .checkboxes-box { margin-left: 145px; }
}

 /*****************************************************************************
 * Contact
 ****************************************************************************/

        .col--contact { 
            h1 {
                margin-bottom: 10px; 
                padding-bottom: 11px; 
                line-height: 120%; 
                color: #333;
                font-weight: bold; 
                @include font-size(16px); 
                background: url('../img/line.gif') no-repeat bottom left;
                }

                address { 
                    @include font-size(12px); 
                    font-style: normal;
                    line-height: 140%; 
                }

                a { 
                    margin-top: 10px;
                    @include font-size(14px); 
                    display: block; 
                }

        }

@include media-query(lap) { 



            .col--contact { 

                .main { margin-top: 1%; }

                h1 {
                    @include font-size(14px); 
                }

                address { 
                    padding-bottom: 15px;
                }

                a { 
                    margin-top: 10px;
                    @include font-size(12px); 
                    display: block; 
                }
            }
}

@include media-query(desk) { 



            .col--contact { 

                margin: 0 0 5px 0;

                .main { margin-top: 0; }

                h1 {
                    @include font-size(12px); 
                }

                address { 
                    padding-bottom: 15px;
                }

                a { 
                    margin-top: 10px;
                    @include font-size(12px); 
                    display: block; 
                }
            }

}

 /*****************************************************************************
 * Login
 ****************************************************************************/

        .page--login {

            .box { background: none; }

            .text-input { 
                margin: 10px 0 10px 24px;
                height: 30px;
                max-width: 320px; 
            }

            .submit-button { margin-left: 0; }

            .sub-title { margin-bottom: 30px !important; }

        }


@include media-query(lap) {

            .page--login {
                form { margin-top: -22px; }

                .box { height: 100px; }

                .submit-button { margin-left: 22%; }

                .text-input { 
                    margin: 0 0 10px 0;
                }
            }


}

@include media-query(desk) {

            .page--login {
                form { margin-top: -28px; }

                .box { height: 100px; }

                .submit-button { margin-left: 120px; }

                .text-input { 
                    margin: 0 0 10px 0;
                }
            }


}



/*
 * Extra responsive junk
 */
@include media-query(palm) { 

    body { 
        font-size: 1.5em;
    }

}

@include media-query(lap) { 

    // body { border: 5px solid green; }

}

@include media-query(desk) { 

    // body { border: 5px solid blue; }

}

